<p id="notice"><%= notice %></p>

<h1>App Settings</h1>

<table>
  <thead>
    <tr>
      <th>Mailer send to email1</th>
      <th>Mailer send to email2</th>
      <th>String 1</th>
      <th>String 2</th>
      <th>String 3</th>
      <th>String 4</th>
      <th>String 5</th>
      <th>Boolian 1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_settings.each do |app_setting| %>
      <tr>
        <td><%= app_setting.mailer_send_to_email1 %></td>
        <td><%= app_setting.mailer_send_to_email2 %></td>
        <td><%= app_setting.string_1 %></td>
        <td><%= app_setting.string_2 %></td>
        <td><%= app_setting.string_3 %></td>
        <td><%= app_setting.string_4 %></td>
        <td><%= app_setting.string_5 %></td>
        <td><%= app_setting.boolian_1 %></td>
        <td><%= link_to 'Show', app_setting %></td>
        <td><%= link_to 'Edit', edit_app_setting_path(app_setting) %></td>
        <td><%= link_to 'Destroy', app_setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Setting', new_app_setting_path %>
