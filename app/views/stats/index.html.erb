<p id="notice"><%= notice %></p>

<h1>Stats</h1>

<table>
  <thead>
    <tr>
      <th>String 1</th>
      <th>String 2</th>
      <th>String 3</th>
      <th>String 4</th>
      <th>String 5</th>
      <th>String num 1</th>
      <th>String num 2</th>
      <th>String num 3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stats.each do |stat| %>
      <tr>
        <td><%= stat.string_1 %></td>
        <td><%= stat.string_2 %></td>
        <td><%= stat.string_3 %></td>
        <td><%= stat.string_4 %></td>
        <td><%= stat.string_5 %></td>
        <td><%= stat.string_num_1 %></td>
        <td><%= stat.string_num_2 %></td>
        <td><%= stat.string_num_3 %></td>
        <td><%= link_to 'Show', stat %></td>
        <td><%= link_to 'Edit', edit_stat_path(stat) %></td>
        <td><%= link_to 'Destroy', stat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stat', new_stat_path %>
