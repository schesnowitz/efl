<h1>Abouts</h1>

<table>
  <thead>
    <tr>
      <th>String 1</th>
      <th>String 2</th>
      <th>String 3</th>
      <th>String 4</th>
      <th>String 5</th>
      <th>String 6</th>
      <th>String 7</th>
      <th>String 8</th>
      <th>String 9</th>
      <th>String 10</th>
      <th>String 11</th>
      <th>String 12</th>
      <th>String 13</th>
      <th>String 14</th>
      <th>String 15</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @abouts.each do |about| %>
      <tr>
        <td><%= about.string_1 %></td>
        <td><%= about.string_2 %></td>
        <td><%= about.string_3 %></td>
        <td><%= about.string_4 %></td>
        <td><%= about.string_5 %></td>
        <td><%= about.string_6 %></td>
        <td><%= about.string_7 %></td>
        <td><%= about.string_8 %></td>
        <td><%= about.string_9 %></td>
        <td><%= about.string_10 %></td>
        <td><%= about.string_11 %></td>
        <td><%= about.string_12 %></td>
        <td><%= about.string_13 %></td>
        <td><%= about.string_14 %></td>
        <td><%= about.string_15 %></td>
        <td><%= link_to 'Show', about %></td>
        <td><%= link_to 'Edit', edit_about_path(about) %></td>
        <td><%= link_to 'Destroy', about, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

